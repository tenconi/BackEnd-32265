<h2 style='color:royalblue'>Bienvenid@ nuevamente
  {{#each userData}}
    {{#if first_name}}
      {{this.first_name}}
    {{/if}}
  {{/each}}</h2>
<br />
<br />

<div class='profile__card'>
  <div class='profile__card--info'>
    {{#each userData}}
      {{#if first_name}}
        <p>name: <span>{{this.first_name}}</span></p>
      {{/if}}
      {{#if last_name}}
        <p>surname: <span>{{this.last_name}}</span></p>
      {{/if}}
      {{#if email}}
        <p>email: <span>{{this.email}}</span></p>
      {{/if}}
      {{#if rol}}
        <p>rol: <span>{{this.rol}}</span></p>
      {{/if}}
    {{/each}}
    </div>

<button onclick="location.href='/user/logout'">LOGOUT</button>

</div>
<br />
<br />
